programa {
  funcao inicio() {

    real vol,larg, pro,comp

    escreva("insira o comprimento da piscina: ")
    leia(comp)

    escreva("insira a largura: ")
    leia(larg)

    escreva("insira a profundidade: ")
    leia(pro)

    vol = (comp * larg * pro )

    escreva("O volume da piscina é de : ",vol," m3 ")
  }
}
